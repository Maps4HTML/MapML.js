      <map-input name="x" type="location" units="tilematrix" axis="column" min="46501"  max="56244" ></map-input>
      <link rel="tile" tref="https://geoappext.nrcan.gc.ca/arcgis/rest/services/BaseMaps/CBMT_CBCT_GEOM_3978/MapServer/tile/{z}/{y}/{x}?m4h=t" />
